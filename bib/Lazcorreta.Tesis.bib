	Note = {\leePDF{./bib/LiuHsuMa-IntegratingClassificationAndARM-1998.pdf} \descarga{http://www.cs.uiuc.edu/class/fa05/cs591han/papers/bliu98.pdf}},
	Pages = {80-86},
	Title = {Integrating Classification and Association Rule Mining},
	Year = {1998}}

