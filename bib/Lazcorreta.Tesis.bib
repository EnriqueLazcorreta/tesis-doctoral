	Journal = {Knowledge-Based Systems},
	Note = {\visita{http://www.sciencedirect.com/science/article/pii/S0950705106001493}},
	Number = {4},
	Pages = {329--335},
	Publisher = {Elsevier Science Publishers B. V.},
	Title = {BitTableFI: An efficient mining frequent itemsets algorithm},
	Volume = {20},
	Year = {2007},
