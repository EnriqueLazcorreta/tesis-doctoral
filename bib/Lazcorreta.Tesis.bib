@article{WangWong-FromAssociationToClassificationInferenceUsingWeightOfEvidence-2003,
	Abstract = {Association and classification are two important tasks in data mining and knowledge discovery. Intensive studies have been carried out in both areas. But, how to apply discovered event associations to classification is still seldom found in current publications. Trying to bridge this gap, this paper extends our previous paper on significant event association discovery to classification. We propose to use weight of evidence to evaluate the evidence of a significant event association in support of, or against, a certain class membership. Traditional weight of evidence in information theory is extended here to measure the event associations of different orders with respect to a certain class. After the discovery of significant event associations inherent in a data set, it is easy and efficient to apply the weight of evidence measure for classifying an observation according to any attribute. With this approach, we achieve flexible prediction.},
	Author = {Wang, Yang and Wong, Andrew KC},
	Date-Added = {2017-05-24 18:43:45 +0000},
	Date-Modified = {2017-05-24 18:43:45 +0000},
	Journal = {Knowledge and Data Engineering, IEEE Transactions on},
	Note = {\visita{http://ieeexplore.ieee.org/xpl/articleDetails.jsp?arnumber=1198405}},
	Number = {3},
	Pages = {764--767},
	Publisher = {IEEE},
	Title = {From association to classification: Inference using weight of evidence},
	Volume = {15},
	Year = {2003}}

@article{ChenZhang-APopularityBasedPredictionModelForWebPrefetching-2003,
	Abstract = {The popularity-based PPM model uses grades to rank URL access patterns and builds these patterns into a predictor tree to aid Web prefetching. In a trace-based simulation, this model was more accurate and used less space than other prediction models.},
	Author = {Chen, Xin and Zhang, Xiaodong},
	Date-Added = {2017-05-24 01:44:00 +0000},
	Date-Modified = {2017-05-24 19:26:18 +0000},
	File = {ChenZhang_APopularityBasedPredictionModelForWebPrefetching_2003.pdf:citas\\ChenZhang_APopularityBasedPredictionModelForWebPrefetching_2003.pdf:PDF},
	Journal = {IEEE Computer},
	Month = {3},
	Note = {\leePDF{./bib/ChenZhang-APopularityBasedPredictionModelForWebPrefetching-2003.pdf} \descarga{http://web.cse.ohio-state.edu/hpcs/WWW/HTML/publications/papers/TR-03-1.pdf} \visita{http://ieeexplore.ieee.org/document/1185219/}},
	Num = {3},
	Owner = {Enrique},
	Pages = {63--70},
	Publisher = {IEEE Computer Society Press},
	Review = {Muy en la línea de nuestro primer artículo. Es conciso y hace una comparativa de los modelos PPM (Prediction by Partial Match), LRS PPM (Longest Repeating Sequences \ldots) y PB PPM (Popularity-Based \ldots). Hacen hincapié en el poco uso de memoria principal con su modelo. Tiene otras ideas interesantes como la inclusión de imágenes en las sesiones si aquellas han sido solicitadas al menos 10 segundos después de la última página accedida por el usuario (ese lapso de tiempo permite pensar que no es una imagen adjunta). Categorizan la frecuencia de cada página (popularidad de la página) en 4 categorías, lo que les sirve para ponderar las páginas a la hora de ser tenidas en cuenta en las sugerencias. También se basan en la {\em probabilidad relativa de solicitar una URL}, cociente de dividir el número de accesos a esa URL entre el número de accesos a su predecesora en la sesión. Usa tres métricas para evaluar los modelos: {\em hit ratio} (también llamado aplicabilidad en \cite{SuYangLuZhang_WhatNext_00}), {\em traffic increment} (ratio entre bytes transmitidos y bytes útiles para el usuario, no lo acabo de comprender) y {\em space} (tamaño de la memoria empleada en el servidor para la predicción, parece que sólo habla de la memoria para el árbol de predicción). Disponible en \url{http://www.cs.wm.edu/hpcs/WWW/HTML/publications/papers/TR-03-1.pdf}. doi: http://dx.doi.org/10.1109/MC.2003.1185219 ISSN = {0018-9162}},
	Timestamp = {2014.11.04},
	Tipo = {predicción},
	Title = {A Popularity-Based Prediction Model for Web Prefetching},
	Vol = {36},
	Year = {2003}}

@inproceedings{SunChenWenyinMa-IntentionModelingForWebNavigation-2002,
	Abstract = {A novel global optimization method referred to as the multi-step dynamic $n$-gram model is proposed for predicting the user's next intended action while he/she is surfing the Web. Unlike the traditional $n$-gram model, in which the predicted action is taken as the ultimate goal and is only determined by its previous $n$ actions, our method predicts the next action that lies on the optimal path that leads to the ultimate goal. Experiments show that the prediction accuracy of our proposed method can achieve up to 3.65\% (or about 11\% relative) improvement to the traditional one-step $n$-gram model.},
	Address = {Beijing, China},
	Annote = {http://research.microsoft.com/en-us/people/zhengc/iciit2002_im.pdf},
	Author = {Sun, Xiaoming and Chen, Zheng and Wenyin, Liu and Ma, Wei-Ying},
